[user]
	name = Jonavon Wilcox
	email = jonavon@gmail.com
[core]
	autocrlf = input 
	excludesfile = ~/.gitignore.global
	preloadindex = true
	fscache = true
[alias]
	st = status
	co = checkout
	cm = commit
	undo = reset HEAD~1 --mixed
	amend = commit -a --amend
	lg = log --graph --decorate --pretty=oneline --abbrev-commit --all
	ll = log --graph --decorate --pretty=oneline --abbrev-commit --all --stat -M
	tree = ! git ls-tree --full-name --name-only -t -r HEAD | sed -e \"s/[^-][^\\/]*\\//   |/g\" -e \"s/|\\([^ ]\\)/|-- \\1/\"
[push]
	default = simple
[gc]
	auto = 256
[init]
	defaultBranch = main
